## validate the phone number

import re

phone_number = '23456645345'
n=re.compile(r'^[7-9]\d{9}$')

if n.match(phone_number):
  print("valid")
else:
  print("invalid")
  
  
  
### validate the email address
##validation using git

import re

email='g.singh.bora@gmail.com'

n = re.compile(r'^[a-zA-Z][\w\-\.]*@[a-zA-Z]+\.[a-zA-Z]{1,3}$')

if n.match(email):
  print("valid")
else:
  print("invalid")
  
  

